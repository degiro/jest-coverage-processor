{
  "name": "jest-coverage-processor",
  "version": "1.1.1",
  "description": "Utils for working with Jest coverage data (auto update thresholds, etc.)",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/degiro/jest-coverage-processor.git"
  },
  "keywords": [
    "Jest",
    "thresholds",
    "auto-update"
  ],
  "author": "degiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/degiro/jest-coverage-processor/issues"
  },
  "homepage": "https://github.com/degiro/jest-coverage-processor#readme",
  "devDependencies": {
    "jest": "^23.6.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/*.js"
    ]
  }
}
